{
  "name": "pidash",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "recharts": "^1.8.5"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -ml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "rescripts": [
    "env"
  ],
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "electron": "^5.0.0",
    "electron-builder": "^22.1.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  },
  "author": {
    "name": "Kelly Bell",
    "email": "kbell109@gmail.com",
    "url": "https://github.com/bellkelly"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "PiDash",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
